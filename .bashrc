alias config='/usr/bin/git --git-dir=/home/leo/.cfg/ --work-tree=/home/leo'

PS1="\e[0;35m\W \$ \e[0m"


alias blt='bluetoothctl'

# View Git status.
alias gs='git status'

# Add a file to Git.
alias ga='git add'

# Commit changes to the code.
alias gc='git commit'

# View the Git log.
alias gl='git log --oneline'

# Start Obsidian Appimage
alias obs="
cd ~/appimages
./obsidian.AppImage
cd ~
"
